{
    "name": "axguowen/think-behaviorcaptcha",
    "description": "Simple Behavior Captcha for ThinkPHP",
    "keywords": [
        "thinkphp",
        "behavior",
        "captcha"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "axguowen",
            "email": "axguowen@qq.com"
        }
    ],
    "require": {
        "topthink/framework": "^6.1||^8.0",
        "axguowen/ajcaptcha-sdk-php": "^1.0||dev-master",
        "axguowen/http-client": "^1.0||dev-master"
    },
    "autoload": {
        "psr-4": {
            "think\\": "src"
        }
    },
    "extra": {
        "think": {
            "config": {
                "behaviorcaptcha": "src/behaviorcaptcha/config.php"
            }
        }
    }
}
